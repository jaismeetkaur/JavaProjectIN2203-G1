class Feline{
String catName;
int catStrength;
public Feline(){
Random rnumber = new Random();
catStrength = rnumber.nextInt(100);
}
}


class Lion extends Feline{
public Lion(){
super();
}
}


class Tiger extends Feline{
public Tiger(){
super();
}
}
